#!/bin/bash

external="${EXTERNAL_DRIVE}"
mountpoint="${EXTERNAL_MOUNT}"

echo
echo "-------------------------------------"
echo " Start jc://remote/${REMOTE_CURRENT_STAGE}/"
echo " (by Christoph Kloth)"
echo "-------------------------------------"


${DOCKERCOMPOSE_DIR}/docker-compose -f ${REMOTE_DIR}/docker-compose.yml up -d

echo
echo "Done."
